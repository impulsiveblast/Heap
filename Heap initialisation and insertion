#include<bits/stdc++.h>
using namespace std;

class heap{
    
    public:
    int arr[1000];
    int size;
    
    heap(){
        size=0;
        arr[size]=-1;
    }
    
    public:
      void insert(int a)
     {
          size=size+1;
          int index=size;
          arr[index]=a;
          
          while(index>1)
          {
              int parent=index/2;
              
              if(arr[parent]<arr[index])
              {
                  swap(arr[parent],arr[index]);
                  
                  index=parent;
                  
              }
              else
              {
                  break;
              }
              
          }
          
     }
          
          void print()
          {
              for(int i=1;i<=size;i++)
              {
                  cout<<arr[i]<<endl;
              }
          }
     
    
     
};
// public:


int main()
{
   heap h1;
   
   h1.insert(7);
   h1.insert(6);
   h1.insert(2);
   h1.insert(10);
   
   
   
   
   h1.print();
   
   
   return 0;
   
}
